# AWS Configuration (for deployment only - NOT passed to Lambda functions)
AWS_REGION=your_AWS_Region
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_access_key_here

# SendZen WhatsApp API Configuration (passed to Lambda functions)
SENDZEN_API_URL=https://api.sendzen.io/v1/messages
SENDZEN_API_KEY=your_sendzen_api_key_here
WHATSAPP_FROM=your_whatsapp_business_number
WHATSAPP_TEMPLATE_NAME=your_template_name
WHATSAPP_LANG_CODE=your_template_language


# Optional: Custom Configuration (passed to Lambda functions)
OTP_EXPIRY_MINUTES=5
MAX_LOGIN_ATTEMPTS=3

# Development Configuration (passed to Lambda functions)
NODE_ENV=development
LOG_LEVEL=info